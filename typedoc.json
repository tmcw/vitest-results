{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["src/index.ts"],
	"basePath": "src/",
	"entryPointStrategy": "resolve",
	"out": "lib/apidoc",
	"excludePrivate": true,
	"excludeExternals": true,
	"excludeInternal": true,
	"treatWarningsAsErrors": true,
	"validation": true,
	"gitRevision": "main"
}
