{
  "$schema": "http://json.schemastore.org/package",
  "name": "@tmcw/vitest-results-neverthrow",
  "version": "1.0.1",
  "type": "module",
  "description": "Vitest matchers for neverthrow Result types",
  "keywords": [
    "vitest",
    "matchers",
    "neverthrow"
  ],
  "license": "MIT",
  "author": "Tom MacWright <tom@macwright.com>",
  "repository": "github:tmcw/vitest-results",
  "homepage": "https://github.com/tmcw/vitest-results",
  "bugs": "https://github.com/tmcw/vitest-results/issues",
  "funding": "https://github.com/tmcw/vitest-results?sponsor=1",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib/",
    "src/"
  ],
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js"
    }
  },
  "scripts": {
    "build": "zshy",
    "test": "biome check && vitest run",
    "prepare": "zshy"
  },
  "zshy": {
    "exports": {
      ".": "./src/index.ts"
    },
    "cjs": false,
    "types": "./src/index.d.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.1.4",
    "@types/chai": "^5.2.2",
    "@types/node": "24.2.0",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/expect": "^3.2.4",
    "chai": "^5.2.1",
    "neverthrow": "^8.2.0",
    "rimraf": "6.0.1",
    "typescript": "5.9.2",
    "vitest": "3.2.4",
    "zshy": "^0.3.5"
  },
  "main": "./lib/index.cjs",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts"
}
